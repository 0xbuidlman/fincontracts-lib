{
  "name": "fincontracts-lib",
  "version": "0.9.1",
  "description": "JavaScript library for interacting with Fincontracts deployed on the Ethereum blockchain",
  "main": "./dist/index.js",
  "scripts": {
    "start": "npm run dev",
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && npm run init",
    "build": "babel ./src -d ./dist --presets es2015,stage-3",
    "publish": "npm run build",
    "test": "xo",
    "docs": "esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asiron/fincontracts-lib.git"
  },
  "author": "asiron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asiron/fincontracts-lib/issues"
  },
  "keywords": [
    "ethereum",
    "financial-derivatives",
    "solidity",
    "dapp"
  ],
  "homepage": "https://github.com/asiron/fincontracts-lib#README.md",
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "esdoc": "^0.5.2",
    "xo": "^0.18.1"
  },
  "dependencies": {
    "mathjs": "^3.9.1",
    "minilog": "^3.1.0",
    "request-promise-native": "^1.0.3"
  },
  "xo": {
    "space": true,
    "rules": {
      "new-cap": 0
    }
  },
  "files": [
    "dist"
  ]
}
